@model IEnumerable<sdcsd.Models.MessageModel>


@{
int i = 0;
foreach (var item in Model.Reverse())
{
    <p class="chat-message">
        <span class="chat-sender">@Html.DisplayFor(modelItem => item.Sender) : </span>
        @Html.DisplayFor(modelItem => item.Content)
    </p>
    
    i++;
    if (i >= 5)
    {
        break;
    }
}
}